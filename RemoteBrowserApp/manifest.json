{
  "name": "Remote Browser",
  "manifest_version":2,  
  "version": "2.1",
  "description": "Gets information from Google.",
  "icons": { "38": "icon38.png" },
  "app":{
	  "background": {/*
		"persistent": false,*/
		"scripts": ["popup.js"]
	  }
  },
  
  "permissions": [
	"http://*.google.com/", 
	"https://*.google.com/",
	"storage"],
  
  "bluetooth":{
	    "uuids": [ "1105", "1106" ],
	  "socket":true
  }
}